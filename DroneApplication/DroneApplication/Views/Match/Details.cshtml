@model IEnumerable<DataAccessLayer.Models.MatchedEventModel>

@{
    ViewData["Title"] = "Details";
}


<h1>Детайли</h1>
<table class="table">
    <thead>
        <tr>
            <th>
                @ViewData["Name"]
            </th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <th>
                Id
            </th>
            <th>
                FileId
            </th>
            <th>
                Name
            </th>
            <th>
                X
            </th>
            <th>
                Y
            </th>
            <th>
                Z
            </th>
        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Id)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.FileId)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.X)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Y)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Z)
                </td>
            </tr>
        }

    </tbody>
</table>
